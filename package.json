{
  "name": "l2c-farm",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start:ci": "serve -s dist -l 3000",
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --noEmit",
    "lint": "eslint '{src,cypress}/**/*.{ts,tsx,js,jsx,json,eslintrc,prettierrc}'",
    "test": "jest --passWithNoTests",
    "test:integration": "cypress run",
    "test:integration:ci": "start-server-and-test start:ci 3000 test:integration",
    "prepare": "husky install"
  },
  "dependencies": {
    "@awsui/components-react": "^3.0.521",
    "@awsui/global-styles": "^1.0.15",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.3",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cypress": "^10.2.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "serve": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,eslintrc,prettierrc,html}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
