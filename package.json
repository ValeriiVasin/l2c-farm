{
  "name": "l2c-farm",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start:ci": "serve -s dist -l 3000",
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --noEmit",
    "lint": "eslint '{src,cypress}/**/*.{ts,tsx,js,jsx,json,eslintrc,prettierrc}'",
    "test": "jest --passWithNoTests",
    "test:integration": "cypress run",
    "test:integration:ci": "start-server-and-test start:ci 3000 test:integration",
    "prepare": "husky install"
  },
  "dependencies": {
    "@awsui/components-react": "^3.0.629",
    "@awsui/global-styles": "^1.0.19",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "recoil": "^0.7.6",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serve": "^13.0.4",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^2.9.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md,eslintrc,prettierrc,html}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
